<%- include('header')-%>

<div class="div-mid blog-title">
  <h2 class="top-pad">Log in to manage posts</h2>
</div>
<div class="div-mid">
  <form class="" action="/login" method="post">
    <br>
    <label for="username">Email</label>
    <input class="tInput" type="text" name="username">
